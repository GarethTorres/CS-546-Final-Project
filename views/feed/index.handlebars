<h1 class="welcomeUser">Welcome {{welcomeUserid}}!</h1>
<div class="row">
    <div class="leftcolumn">
        <div id="showcaseColumn">
            {{#each allShowcases}} {{> showcase }} {{/each}}
        </div>
        <a href="" id="fetchMoreShowcases">Get more showcases</a>
        <p id="noMoreShowcases" hidden>No more showcases!</p>
    </div>
    <div class="rightcolumn">
        <h2>Following:</h2>
        <ul>
        {{#each followingUsers}}
            <li><a href="/users/profile/{{this.[1]}}">{{this.[0]}}</a></li>
        {{/each}}
        </ul>
    </div>
</div>
<script src="/public/js/feed.js"></script>
